<!-- Imports -->
@model SadenaFenix.Models.Usuarios.Usuario

<!-- Scope -->
@{
    ViewData["Root"] = "Sadena";
    ViewData["Version"] = "1.0.0";
    ViewData["Year"] = "2018";
}
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - @ViewData["Root"]</title>

    <!-- Styles -->
    <link rel="stylesheet" href="~/Scripts/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/Scripts/lib/font-awesome/css/font-awesome.min.css" />
    <link rel="stylesheet" href="~/Scripts/lib/datatables.net-bs/css/dataTables.bootstrap.min.css" />
    <link rel="stylesheet" href="~/Scripts/lib/select2/dist/css/select2.min.css" />
    <link rel="stylesheet" href="~/Scripts/lib/bootswatch/3/simplex/bootstrap.min.css" />
    <link rel="stylesheet" href="~/Scripts/lib/jquery-jvectormap/jquery-jvectormap-2.0.3.min.css" />
    <link rel="stylesheet" href="~/Content/css/site.css" />

</head>
<body>

    <!-- Navegation -->
    <nav class="navbar navbar-default navbar-fixed-top">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#bs-example-navbar-collapse-2">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <!-- Brand -->
                <a class="navbar-brand" href="/Home/Index">@ViewData["Root"]</a>
            </div>
            <!-- Menu-->
            <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-2">
                <ul class="nav navbar-nav">
                    <li class="dropdown" style="display:block;">
                        <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-expanded="false">
                            <span>Nacimientos <span class="caret"></span></span>
                        </a>
                        <ul class="dropdown-menu" role="menu">
                            <li style="display:block;">
                                <a href="/Archivos/Importar">Importar archivos</a>
                            </li>
                            <li class="divider" style="display:block;"></li>
                            <li><a href="/Consultas/Consultar">Consultar</a></li>
                            <li><a href="/Reportes/Reportes">Reportes</a></li>
                        </ul>
                    </li>
                    <li class="dropdown" style="display:block;">
                        <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-expanded="false">
                            <span>Georeferenciación <span class="caret"></span></span>
                        </a>
                        <ul class="dropdown-menu" role="menu">
                            <li style="display:block;">
                                <a href="/Oficialias/Oficialias">Oficialías</a>
                            </li>
                            <li><a href="/Hospitales/Hospitales">Hospitales</a></li>
                            <li><a href="/ModulosHospitalarios/ModulosHospitalarios">Módulos Hospitalarios</a></li>
                        </ul>
                    </li>
                </ul>
                <!-- Right menu -->
                <ul class="nav navbar-nav navbar-right">
                    <li><a href="/Home/About">Acerca de</a></li>
                    <li><a href="/Home/Contact">Contacto</a></li>
                    <li class="dropdown" style="display:block;">
                        <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-expanded="false">
                            <span><i class="fa fa-user-circle-o"></i></span>
                        </a>
                        <!-- User's menu -->
                        <ul class="dropdown-menu" role="menu">
                            <li>
                                <!-- Session's info -->
                                <div class="col-sm-12">
                                    <h5>
                                        <strong>Adminstrador @Html.DisplayFor(model => model.Rol.RolDesc)</strong>
                                        <br /><small>admin@sadena.gob.mx @Html.DisplayFor(model => model.CorreoE)</small>
                                    </h5>
                                </div>
                            </li>
                            <li>
                                <a href="/Acceso/Salir">
                                    <span><i class="fa fa-sign-out mr-3"></i>Salir</span>
                                </a>
                            </li>
                        </ul>
                    </li>
                </ul>
            </div>
        </div>
    </nav>
    <!-- ./navegation -->
    <!-- Main container -->
    <div class="container-fluid">
        @RenderBody()

        <!-- Footer -->
        <div class="footer-container">
            <hr />
            <footer>
                <!-- Version -->
                <div class="pull-right hidden-xs">
                    <b>Versión</b> @ViewData["Version"]
                </div>
                <!-- Default -->
                <strong>
                    Copyright &copy; @ViewData["Year"] -
                    <a href="/Home/Index">@ViewData["Root"]</a>.
                </strong> Todos los derechos reservados.
            </footer>
        </div>
        <!-- ./footer -->

    </div>
    <!-- ./main container -->
    <!-- Modal -->
    <div id="defaultModal" class="modal fade" role="dialog">
        <div class="modal-dialog modal-lg">
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
                    <h4 class="modal-title">Mensaje</h4>
                </div>
                <div class="modal-body">
                    <p id="defaultModalMessage"></p>
                </div>
                <div class="modal-footer">
                    <form action="/Nacimientos/Comparar">
                        <button type="submit" class="btn btn-default" data-dismiss="modal">Continuar</button>
                    </form>
                </div>
            </div>
        </div>
    </div>
    <!-- ./modal -->

    <partial name="_CookieConsentPartial"></partial>

    <!-- Scripts -->
    <script src="~/Scripts/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/Scripts/lib/bootstrap/dist/js/bootstrap.min.js"></script>
    <script src="~/Scripts/lib/datatables.net/js/jquery.dataTables.min.js"></script>
    <script src="~/Scripts/lib/select2/dist/js/select2.full.min.js"></script>
    <script src="~/Scripts/lib/jquery-jvectormap/jquery-jvectormap-2.0.3.min.js"></script>
    <script src="~/Scripts/js/maps/jqvmap/jquery.vmap.municipios_coahuila.js"></script>
    <script src="~/Scripts/js/site.js"></script>
    <script src="~/Scripts/js/nacimientos/reportes/reportes.js"></script>
    <script src="~/Scripts/js/nacimientos/consultas/consultar.js"></script>
    <script src="~/Scripts/js/nacimientos/consultas/importar.js"></script>

    @RenderSection("Scripts", required: false)

</body>
</html>
