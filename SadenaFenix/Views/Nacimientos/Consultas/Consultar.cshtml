@model SadenaFenix.Transport.Nacimientos.Consultas.ConsultasViewModel

<!-- Scope -->
@{
    ViewData["Root"] = "Sadena";
    ViewData["Modulo"] = "Nacimientos";
    ViewData["Option"] = "Consultas";
    ViewData["Title"] = ViewData["Modulo"] + " / " + ViewData["Option"];
}

<!-- Page content -->
<div class="page-content">

    @Html.HiddenFor(model => model.Usuario.Json, new { id = "objUsuario" })

    <!-- Content -->
    <div class="container">

        <!-- Breadcrumb -->
        <section class="pull-right">
            <ul class="breadcrumb">
                <li><a href="/Home/Index"><i class="fa fa-home mr-3"></i>@ViewData["Root"]</a></li>
                <li><a href="#">@ViewData["Modulo"]</a></li>
                <li class="active">@ViewData["Option"]</li>
            </ul>
        </section>
        <!-- ./breadcrumb -->
        <!-- Search -->
        <section>
            <div class="row">
                <div class="col-sm-12">
                    <div class="well">
                        <form class="form-horizontal">
                            <fieldset>
                                <legend>Opciones de búsqueda</legend>
                                <div id="#inputListaSeleccionMeses" class="form-group">
                                    <label for="MesesSeleccionados">Mes</label>
                                    @Html.DropDownListFor(model => model.ComboMeses.MesesSeleccionados, new SelectList(Model.ComboMeses.Meses, "Value", "Text"), htmlAttributes: new { @class = "form-control select2", multiple = "", title = "Meses seleccionados" })
                                </div>
                                <!-- /.form-group -->
                                <div id="#inputListaSeleccionAnios" class="form-group">
                                    <label for="AniosSeleccionados">Año</label>
                                    @Html.DropDownListFor(model => model.ComboAnios.AniosSeleccionados, new SelectList(Model.ComboAnios.Anios, "Value", "Text"), htmlAttributes: new { @class = "form-control select2", multiple = "", title="Años seleccionados" })
                                </div>
                                <!-- /.form-group -->
                                <div id="#inputListaSeleccionMunicipios" class="form-group">
                                    <label for="MunicipiosSeleccionados">Municipios</label>
                                    @Html.DropDownListFor(model => model.ComboMunicipios.MunicipiosSeleccionados, new SelectList(Model.ComboMunicipios.Municipios, "Value", "Text"), htmlAttributes: new { @class = "form-control select2", multiple = "", title="Municipios seleccionados" })
                                </div>
                                <!-- /.form-group -->
                                <br />
                                <div class="form-group" style="padding-bottom:10px;">
                                    <div class="btn-group">
                                        <button id="consultarDatosProcesadosBtn" type="button" class="btn btn-success" title="Buscar información procesada">Buscar <i class="fa fa-search"></i></button>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-sm-12">
                                        <p class="note-feet"><strong>* Seleccione el periodo y los municipios (Si se dejan en blanco, se realizará el cálculo general: "todos los registros de la base de datos").</strong></p>
                                    </div>
                                </div>
                                <!-- /.form-group -->
                            </fieldset>
                        </form>
                    </div>
                </div>
            </div>
        </section>
        <!-- ./search -->
        <!-- Result -->
        <section id="cantidadesResumen" style="display:none;">
            <div class="row">
                <div class="col-sm-12">
                    <!-- Panel -->
                    <div class="panel panel-default">
                        <!-- Heading -->
                        <div class="panel-heading">
                            <span class="panel-title">Resultados de la comparación</span>
                        </div>
                        <!-- ./heading -->
                        <!-- Body -->
                        <div class="panel-body">
                            <div class="row">
                                <div class="col-sm-3">
                                    <div class="description-block">
                                        <span id="PorcentajeSubregistro" class="badge badge-primary"></span>
                                        <h5 id="TotalSubregistro"></h5>
                                        <a href="#subregistradosTab" class="consultaDetalleSubRegistro" data-toggle="tab" aria-expanded="true">
                                            <span>Subregistro</span>
                                        </a>
                                    </div>
                                </div>
                                <!-- /.col -->
                                <div class="col-sm-3">
                                    <div class="description-block">
                                        <span id="PorcentajeRegistroOportuno" class="badge badge-success"></span>
                                        <h5 id="TotalRegistroOportuno"></h5>
                                        <a href="#registradosTab" class="consultaDetalleSubRegistro" data-toggle="tab" aria-expanded="true">
                                            <span>Registro Oportuno</span>
                                        </a>
                                    </div>
                                </div>
                                <div class="col-sm-3">
                                    <div class="description-block">
                                        <span id="PorcentajeRegistroExtemporaneo" class="badge badge-warning">20%</span>
                                        <h5 id="TotalRegistroExtemporaneo"></h5>
                                        <a href="#extemporaneosTab" class="consultaDetalleSubRegistro" data-toggle="tab" aria-expanded="true">
                                            <span>Registro Extemporáneo</span>
                                        </a>
                                    </div>
                                </div>
                                <!-- /.col -->
                                <div class="col-sm-3">
                                    <div class="description-block">
                                        <br /><span><i class="fa fa-danger fa-line-chart"></i></span>
                                        <br /><a id="ResumenTotales"
                                               href="#resumenTotalesTab" data-toggle="tab" aria-expanded="true">
                                            <span>Resumen por municipio</span>
                                        </a>
                                    </div>
                                </div>
                                <!-- /.col -->
                            </div>
                            <!-- /.row -->
                        </div>
                        <!-- ./body -->
                        <div class="panel-footer">
                            <div class="tab-content">
                                <!-- Results -->
                                <div id="subregistradosTab" class="row tab-pane fade">
                                    <div class="col-sm-12">
                                        <p class="text-center">
                                            <strong>Nacimientos con subregistro</strong></p>
                                        <div class="table-container">
                                            <table id="subregistradosTable" class="table table-striped table-hover">
                                                <thead>
                                                    <tr>
                                                        <th>FOLIO</th>
                                                        <th colspan="3" style="border-top: 2px solid #dddddd;">DATOS DE LA MADRE</th>
                                                        <th colspan="9" style="border-top: 2px solid #dddddd;">DATOS DE LA RESIDENCIA DE LA MADRE</th>
                                                        <th colspan="4" style="border-top: 2px solid #dddddd;">DATOS SOCIOECONÓMICOS DE LA MADRE</th>
                                                        <th colspan="4" style="border-top: 2px solid #dddddd;">DATOS DEL RECIEN NACIDO</th>
                                                    </tr>
                                                    <tr>
                                                        <th>Folio del certificado de nacimiento</th>
                                                        <th>Edad</th>
                                                        <th>ID Estado civil</th>
                                                        <th>Estado civil</th>
                                                        <th>Calle</th>
                                                        <th>Número exterior</th>
                                                        <th>Número interior</th>
                                                        <th>ID Entidad</th>
                                                        <th>Entidad</th>
                                                        <th>ID Municipio</th>
                                                        <th>Municipio</th>
                                                        <th>ID Localidad</th>
                                                        <th>Localidad</th>
                                                        <th>Número de nacimiento vivo</th>
                                                        <th>ID Escolaridad</th>
                                                        <th>Escolaridad</th>
                                                        <th>Ocupación</th>
                                                        <th>Fecha de nacimiento</th>
                                                        <th>Hora de nacimiento</th>
                                                        <th>ID Sexo</th>
                                                        <th>Sexo</th>
                                                    </tr>
                                                </thead>
                                                <tbody>
                                                    <!-- Records are fulling by AJAX -->
                                                </tbody>
                                            </table>
                                            <!-- ./table -->
                                        </div>
                                    </div>
                                    <!-- ./col -->
                                </div>
                                <div id="registradosTab" class="row tab-pane fade">
                                    <div class="col-sm-12">
                                        <p class="text-center">
                                            <strong>Nacimientos con registro</strong></p>
                                        <div class="table-container">
                                            <table id="registradosTable" class="table table-striped table-hover">
                                                <thead>
                                                    <tr>
                                                        <th style="border-top: 2px solid #dddddd;">FOLIO</th>
                                                        <th colspan="3" style="border-top: 2px solid #dddddd;">DATOS DE LA MADRE</th>
                                                        <th colspan="9" style="border-top: 2px solid #dddddd;">DATOS DE LA RESIDENCIA DE LA MADRE</th>
                                                        <th colspan="4" style="border-top: 2px solid #dddddd;">DATOS SOCIOECONÓMICOS DE LA MADRE</th>
                                                        <th colspan="7" style="border-top: 2px solid #dddddd;">DATOS DEL RECIEN NACIDO</th>
                                                    </tr>
                                                    <tr>
                                                        <th>Folio del certificado de nacimiento</th>
                                                        <th>Edad</th>
                                                        <th>ID Estado civil</th>
                                                        <th>Estado civil</th>
                                                        <th>Calle</th>
                                                        <th>Número exterior</th>
                                                        <th>Número interior</th>
                                                        <th>ID Entidad</th>
                                                        <th>Entidad</th>
                                                        <th>ID Municipio</th>
                                                        <th>Municipio</th>
                                                        <th>ID Localidad</th>
                                                        <th>Localidad</th>
                                                        <th>Número de nacimiento vivo</th>
                                                        <th>ID Escolaridad</th>
                                                        <th>Escolaridad</th>
                                                        <th>Ocupación</th>
                                                        <th>Fecha de nacimiento</th>
                                                        <th>Hora de nacimiento</th>
                                                        <th>ID Sexo</th>
                                                        <th>Sexo</th>
                                                        <th>Fecha de registro (según folio)</th>
                                                        <th>Fecha de registro (según fecha)</th>
                                                        <th>Días al nacimiento</th>
                                                    </tr>
                                                </thead>
                                                <tbody>
                                                    <!-- Records are fulling by AJAX -->
                                                </tbody>
                                            </table>
                                            <!-- ./table -->
                                        </div>
                                    </div>
                                    <!-- ./col -->
                                </div>
                                <div id="extemporaneosTab" class="row tab-pane fade">
                                    <div class="col-sm-12">
                                        <p class="text-center">
                                            <strong>Nacimientos con registro extemporáneo</strong></p>
                                        <div class="table-container">
                                            <table id="extemporaneosTable" class="table table-striped table-hover">
                                                <thead>
                                                    <tr>
                                                        <th style="border-top: 2px solid #dddddd;">FOLIO</th>
                                                        <th colspan="3" style="border-top: 2px solid #dddddd;">DATOS DE LA MADRE</th>
                                                        <th colspan="9" style="border-top: 2px solid #dddddd;">DATOS DE LA RESIDENCIA DE LA MADRE</th>
                                                        <th colspan="4" style="border-top: 2px solid #dddddd;">DATOS SOCIOECONÓMICOS DE LA MADRE</th>
                                                        <th colspan="7" style="border-top: 2px solid #dddddd;">DATOS DEL RECIEN NACIDO</th>
                                                    </tr>
                                                    <tr>
                                                        <th>Folio del certificado de nacimiento</th>
                                                        <th>Edad</th>
                                                        <th>ID Estado civil</th>
                                                        <th>Estado civil</th>
                                                        <th>Calle</th>
                                                        <th>Número exterior</th>
                                                        <th>Número interior</th>
                                                        <th>ID Entidad</th>
                                                        <th>Entidad</th>
                                                        <th>ID Municipio</th>
                                                        <th>Municipio</th>
                                                        <th>ID Localidad</th>
                                                        <th>Localidad</th>
                                                        <th>Número de nacimiento vivo</th>
                                                        <th>ID Escolaridad</th>
                                                        <th>Escolaridad</th>
                                                        <th>Ocupación</th>
                                                        <th>Fecha de nacimiento</th>
                                                        <th>Hora de nacimiento</th>
                                                        <th>ID Sexo</th>
                                                        <th>Sexo</th>
                                                        <th>Fecha de registro (según folio)</th>
                                                        <th>Fecha de registro (según fecha)</th>
                                                        <th>Días al nacimiento</th>
                                                    </tr>
                                                </thead>
                                                <tbody>
                                                    <!-- Records are fulling by AJAX -->
                                                </tbody>
                                            </table>
                                            <!-- ./table -->
                                        </div>
                                    </div>
                                    <!-- ./col -->
                                </div>
                                <div id="resumenTotalesTab" class="row tab-pane fade">
                                    <div class="col-sm-12">
                                        <p class="text-center">
                                            <strong>Resumen por municipio</strong>
                                        </p>
                                        <div class="table-container">
                                            <table id="resumenTotalesTable" class="table table-striped table-hover">
                                                <thead>
                                                    <tr>
                                                        <th style="border-top: 2px solid #dddddd;">ID Municipio</th>
                                                        <th style="border-top: 2px solid #dddddd;">Municipio</th>
                                                        <th style="border-top: 2px solid #dddddd;">Folios sin Registro</th>
                                                        <th style="border-top: 2px solid #dddddd;">Folios con Registro</th>
                                                        <th style="border-top: 2px solid #dddddd;">Folios con Subregistro</th>
                                                    </tr>
                                                </thead>
                                                <tbody>
                                                    <!-- Records are fulling by AJAX -->
                                                </tbody>
                                            </table>
                                            <!-- ./table -->
                                        </div>
                                    </div>
                                    <!-- ./col -->
                                </div>
                            </div>
                        </div>
                        <!-- ./footer -->
                    </div>
                    <!-- ./panel -->
                </div>
                <!-- ./col -->
            </div>
            <!-- ./row -->
        </section>
        <!-- ./result -->

    </div>
    <!-- ./content -->

</div>
<!-- ./page content -->