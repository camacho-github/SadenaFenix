@model dynamic

<input type="hidden" name="objTotalesCovertura" id="objTotalesCovertura" value="@Model.Totales" />

<div id="ROportunoSinRelacion">
    <div id="OportunoSinRelacionTableContainer" class="table-container table-responsive">
        <div class="col-sm-12">
            <div id="OportunoSinRelacionTableBottonsContainer" class="btn-group btn-group-justified">
                <div class="table-responsive">
                    <table id="OportunoSinRelacionTabla" class="table table-condensed table-striped table-bordered" style="position:relative">
                        <thead>
                            <tr>
                                @for (int j = 0; j < Model.OportunoSinRelacion.Columns.Count; j++)
                                {
                                    <th>@Model.OportunoSinRelacion.Columns[j]</th>
                                }
                            </tr>
                        </thead>
                        <tbody>
                            @for (int i = 0; i < Model.OportunoSinRelacion.Rows.Count; i++)
                            {
                                <tr class=@Model.OportunoSinRelacion.Rows[i][0]>
                                    @for (int j = 0; j < Model.OportunoSinRelacion.Columns.Count; j++)
                                    {
                                        <td>@Model.OportunoSinRelacion.Rows[i][j]</td>
                                    }
                                </tr>
                            }
                        </tbody>
                        <tfoot>
                            <tr>
                                @for (int j = 0; j < Model.OportunoSinRelacion.Columns.Count; j++)
                                {
                                    <th>@Model.OportunoSinRelacion.Columns[j]</th>
                                }
                            </tr>
                        </tfoot>
                    </table>
                </div>

            </div>
        </div>
    </div>
</div>


