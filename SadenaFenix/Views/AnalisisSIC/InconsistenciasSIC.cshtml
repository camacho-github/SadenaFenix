@model dynamic

<input type="hidden" name="totalCaracteresEspeciales" id="totalCaracteresEspeciales" value="@Model.CaracteresEspeciales.Rows.Count" />

<div id="RCaracteresEspeciales">
    <div id="CaracteresEspecialesContainer" class="table-container table-responsive">
        <div class="col-sm-12">
            <div id="CaracteresEspecialesBottonsContainer" class="btn-group btn-group-justified">
                <div class="table-responsive">
                    <table id="CaracteresEspecialesTabla" class="table table-condensed table-striped table-bordered" style="position:relative">
                        <thead>
                            <tr>
                                @for (int j = 0; j < Model.CaracteresEspeciales.Columns.Count; j++)
                                {
                                    <th>@Model.CaracteresEspeciales.Columns[j]</th>
                                }
                            </tr>
                        </thead>
                        <tbody>
                            @for (int i = 0; i < Model.CaracteresEspeciales.Rows.Count; i++)
                            {
                                <tr class=@Model.CaracteresEspeciales.Rows[i][0]>
                                    @for (int j = 0; j < Model.CaracteresEspeciales.Columns.Count; j++)
                                    {
                                        <td>@Model.CaracteresEspeciales.Rows[i][j]</td>
                                    }
                                </tr>
                            }
                        </tbody>
                        <tfoot>
                            <tr>
                                @for (int j = 0; j < Model.CaracteresEspeciales.Columns.Count; j++)
                                {
                                    <th>@Model.CaracteresEspeciales.Columns[j]</th>
                                }
                            </tr>
                        </tfoot>
                    </table>
                </div>

            </div>
        </div>
    </div>
</div>


