                            {
                                <tr class=@Model.DuplicadosSIC.Rows[i][0]>
                                    @for (int j = 0; j < Model.DuplicadosSIC.Columns.Count; j++)
                                    {
                                        <td>@Model.DuplicadosSIC.Rows[i][j]</td>
