@model dynamic

<div id="REdadSub">
    <div id="subregistradosTableContainer" class="table-container table-responsive">
        <div class="col-sm-12">
            <div id="subregistradosTableBottonsContainer" class="btn-group btn-group-justified">
                <div class="table-responsive">
                    <table id="SubRegistroTabla" class="table table-condensed table-striped table-bordered" style="position:relative">
                        <thead>
                            <tr>
                                @for (int j = 0; j < Model.ReporteSubRegistros.Columns.Count; j++)
                                {
                                    <th>@Model.ReporteSubRegistros.Columns[j]</th>
                                }
                            </tr>
                        </thead>
                        <tbody>
                            @for (int i = 0; i < Model.ReporteSubRegistros.Rows.Count; i++)
                            {
                                <tr class=@Model.ReporteSubRegistros.Rows[i][0]>
                                    @for (int j = 0; j < Model.ReporteSubRegistros.Columns.Count; j++)
                                    {
                                        <td>@Model.ReporteSubRegistros.Rows[i][j]</td>
                                    }
                                </tr>
                            }
                        </tbody>
                        <tfoot>
                            <tr>
                                @for (int j = 0; j < Model.ReporteSubRegistros.Columns.Count; j++)
                                {
                                    <th>@Model.ReporteSubRegistros.Columns[j]</th>
                                }
                            </tr>
                        </tfoot>
                    </table>
                </div>

            </div>
        </div>
    </div>
</div>
                            

